{
  "version": 2,
  "builds": [
    {
      "src": "docs/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "docs"
      }
    }
  ],
  "routes": [
    { "src": "/about", "dest": "/docs/about.html" },
    { "src": "/payment", "dest": "/docs/payment.html" },
    { "src": "/payment-page", "dest": "/docs/payment-page.html" },
    { "src": "/contact", "dest": "/docs/contact.html" },
    { "src": "/venue", "dest": "/docs/venue.html" },
    { "src": "/committee", "dest": "/docs/committee.html" },
    { "src": "/(.*)", "dest": "/docs/index.html" }
  ]
}
